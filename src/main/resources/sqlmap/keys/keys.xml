<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hwj.modules.keys.dao.KeysDao">
	<resultMap type="com.hwj.modules.keys.model.KeysModel" id="KeysModelMap">
		<result property="id" column="ID" jdbcType="VARCHAR" />
		<result property="key" column="KEY" jdbcType="VARCHAR" />
		<result property="desc" column="DESCRIPTION" jdbcType="VARCHAR" />
		<result property="rawDesc" column="RAW_DESC" jdbcType="VARCHAR" />
	</resultMap>

	<select id="search" parameterType="map" resultMap="KeysModelMap">
		select
		t.id,t.key,t.raw_desc,t.description
		from
		keys t
		order by last_modify_time desc
		<where>
		</where>
	</select>

	<select id="checkExist" parameterType="string" resultType="int">
		select
		count(1)
		from
		keys t
		where t.key = #{key}
		<where>
		</where>
	</select>

	<insert id="addOne" parameterType="map">
		insert into
		keys
		(key,raw_desc,description,create_time)
		values
		(#{key},#{rawDesc},#{desc},sysdate)
	</insert>

	<update id="edit">
		update keys
		set
		key = #{key},
		raw_desc = #{rawDesc},
		description = #{desc}
		where
		id = #{id}
	</update>

	<delete id="delete" parameterType="string">
		delete from keys where id = #{id}
	</delete>

	<delete id="clearTable">
		delete from keys
	</delete>

</mapper>